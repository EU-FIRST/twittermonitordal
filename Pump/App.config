<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="InputConnectionString" value="Server=(local);Database=TwitterDatasetHistorical;Trusted_Connection=Yes"/>
    <add key="OutputConnectionString" value="Server=(local);Database=TwitterDataset;Trusted_Connection=Yes"/>
    <add key="CommandTimeout" value="0"/>
    <add key="InputSelectStatement" 
         value="select Id, Text, CreatedAt from tweets t join tweettoquery ttq on ttq.tweetid = t.id where queryid = '424B5F7B-1347-8D5F-8763-E11B4B292E1C' and createdat >= '2011-12-01' and createdat &lt; '2013-01-01' and id > @Id order by id"/>
    <add key="TaggedWords" value="$LDSH,LDSH"/>
    <add key="Topic" value="LDSH"/>
    <add key="OutputTableNameSuffix" value="LDSH"/>
    <add key="SleepSeconds" value="900"/> <!-- 15 minutes -->
    <add key="WindowSizeMinutes" value="10080"/>
    <add key="StepSizeMinutes" value="60"/>
    <add key="ClusterQualityThresh" value="0.2"/>
    <add key="SaveStateNumSteps" value="2147483000"/>
  </appSettings>
</configuration>