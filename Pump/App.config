<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="InputConnectionString" value="Server=(local);Database=TwitterDataset;Trusted_Connection=Yes"/>
    <add key="OutputConnectionString" value="Server=(local);Database=TwitterDataset;Trusted_Connection=Yes"/>
    <add key="CommandTimeout" value="0"/>
    <add key="InputSelectStatement" 
         value="SELECT Tweets.TweetId AS Id, TweetText AS Text, DateTime AS CreatedAt FROM Tweets JOIN Stocks ON Tweets.TweetId = Stocks.TweetId WHERE StockSymbol = '$AAPL' ORDER BY DateTime"/>
  </appSettings>
</configuration>