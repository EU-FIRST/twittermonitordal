<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="InputConnectionString" value="Server=(local);Database=TwitterDatasetHistorical;Trusted_Connection=Yes"/>
    <add key="OutputConnectionString" value="Server=(local);Database=TwitterDataset;Trusted_Connection=Yes"/>
    <add key="CommandTimeout" value="0"/>
    <add key="InputSelectStatement" 
         value="select Id, Text, CreatedAt from tweets t join tweettoquery ttq on ttq.tweetid = t.id where queryid = 'FC3906D9-4E09-F0A4-7389-28E677360198' and createdat >= '2011-12-01' and createdat &lt; '2013-01-01' and id > @Id order by id"/>
    <add key="TaggedWords" value="$GOOG,GOOG"/>
    <add key="Topic" value="GOOG"/>
    <add key="OutputTableNameSuffix" value="GOOG"/>
    <add key="SleepSeconds" value="900"/> <!-- 15 minutes -->
    <add key="WindowSizeMinutes" value="10080"/>
    <add key="StepSizeMinutes" value="60"/>
    <add key="ClusterQualityThresh" value="0.2"/>
    <add key="SaveStateNumSteps" value="5"/>
  </appSettings>
</configuration>